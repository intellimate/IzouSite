{
  "name": "izouwebapp",
  "version": "0.0.1",
  "description": "The front-end website of Izou. This is where anything pertaining to Izou can be managed.",
  "main": "index.html",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/intellimate/IzouWebApp.git"
  },
  "keywords": [
    "izou",
    "izousite",
    "front-end",
    "website"
  ],
  "author": "Intellimate",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/intellimate/IzouWebApp/issues"
  },
  "homepage": "https://github.com/intellimate/IzouWebApp#readme",
  "engines": {
    "npm": ">=3.0.0",
    "node": ">=5.0.0"
  },
  "dependencies": {
    "axios": "^0.11.1",
    "history": "^2.1.1",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-router": "^2.4.1",
    "redux": "^3.5.2"
  },
  "devDependencies": {
    "babel-cli": "6.7.7",
    "babel-preset-es2015-rollup": "1.1.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "browser-sync": "2.12.5",
    "chai": "^3.5.0",
    "gh-pages": "^0.11.0",
    "mocha": "^2.5.3",
    "npm-run-all": "1.6.0",
    "onchange": "2.2.0",
    "rollup": "0.26.1",
    "rollup-plugin-babel": "2.4.0",
    "rollup-plugin-commonjs": "2.2.1",
    "rollup-plugin-node-globals": "1.0.5",
    "rollup-plugin-node-resolve": "1.5.0",
    "rollup-plugin-npm": "1.4.0",
    "rollup-plugin-replace": "1.1.0",
    "rollup-plugin-uglify": "0.3.1",
    "snazzy": "3.0.1",
    "standard": "6.0.8"
  },
  "scripts": {
    "prebuild": "npm run std",
    "build": "rollup -c rollup.config.prod.js",
    "build:dev": "rollup -c rollup.config.dev.js",
    "browse": "browser-sync start --s --index 'src/html/index.html' --files 'src/**/*.html, src/**/*.js' --no-notify",
    "std": "standard --verbose | snazzy",
    "test": "mocha test",
    "watch": "onchange src -- npm run build:dev",
    "preversion": "git pull && npm up && npm run std",
    "version": "npm run build && git add -A .",
    "postversion": "git push --tags origin HEAD"
  },
  "standard": {
    "ignore": [
      "build",
      "node_modules",
      "test"
    ]
  }
}
