{
  "name": "izouwebapp",
  "version": "0.0.1",
  "description": "The front-end website of Izou. This is where anything pertaining to Izou can be managed.",
  "main": "index.html",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/intellimate/IzouWebApp.git"
  },
  "keywords": [
    "izou",
    "izousite",
    "front-end",
    "website"
  ],
  "author": "Intellimate",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/intellimate/IzouWebApp/issues"
  },
  "homepage": "https://github.com/intellimate/IzouWebApp#readme",
  "engines": {
    "npm": ">=3.0.0",
    "node": ">=5.0.0"
  },
  "dependencies": {
    "axios": "^0.11.1",
    "babel-polyfill": "^6.9.1",
    "history": "^2.1.1",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "redux": "^3.5.2"
  },
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "cross-env": "^1.0.8",
    "gh-pages": "^0.11.0",
    "mocha": "^2.5.3",
    "npm-run-all": "1.6.0",
    "react-addons-test-utils": "^15.1.0",
    "react-hot-loader": "^1.3.0",
    "snazzy": "3.0.1",
    "standard": "6.0.8",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "scripts": {
    "prebuild": "npm run std",
    "build": "webpack --config config/webpack.config.prod.js",
    "build:dev": "webpack --config config/webpack.config.dev.js",
    "dev": "webpack-dev-server --content-base src",
    "std": "standard --verbose | snazzy",
    "test": "mocha test",
    "watch": "webpack --config config/webpack.config.dev.js --watch",
    "preversion": "git pull && npm up && npm run std",
    "version": "npm run build && git add -A .",
    "postversion": "git push --tags origin HEAD"
  },
  "standard": {
    "ignore": [
      "build",
      "node_modules"
    ]
  }
}
